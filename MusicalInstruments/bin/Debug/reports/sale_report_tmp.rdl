        </Field>
        <Field Name="PRICE">
          <DataField>PRICE</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="END_PRICE">
          <DataField>END_PRICE</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
